{
  "name": "sdc-ui",
  "version": "1.4.2",
  "description": "This project aims to create a unified UI styled components for multiple development teams who work on the same web-based applications.",
  "main": "lib/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage && start ./coverage/lcov-report/index.html",
    "build": "babel src/react -d lib/react && babel src/index.js -o lib/index.js && node-sass --include-path scss scss/style.scss css/style.css && node build-demo.js",
    "storybook": "start-storybook -p 6006 -s ./assests/icons",
    "build-storybook": "build-storybook -c .storybook -o .out -s ./assests/icons",
    "lab": "ng2-component-lab --config .ng2-component-lab/ng2-component-lab.config.js -- feature"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onap-sdc/sdc-ui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onap-sdc/sdc-ui/issues"
  },
  "files": [
    "lib",
    "css"
  ],
  "homepage": "https://github.com/onap-sdc/sdc-ui#readme",
  "devDependencies": {
    "@islavi/ng2-component-lab": "^1.0.3",
    "@types/node": "^7.0.27",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "copy": "^0.3.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "html-loader": "^0.4.5",
    "jest": "^20.0.0",
    "node-sass": "^4.5.3",
    "prismjs": "^1.6.0",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "typescript": "^2.3.4",
    "url-parse": "^1.1.9"
  },
  "dependencies": {
    "@angular/common": "~2.2.1",
    "@angular/compiler": "~2.2.1",
    "@angular/core": "~2.2.1",
    "@angular/platform-browser": "~2.2.1",
    "@angular/platform-browser-dynamic": "~2.2.1",
    "@angular/router": "~3.2.1",
    "@storybook/react": "^3.1.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.26"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "html"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.html$": "<rootDir>/test/utils/htmlLoader.js"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/react/index.js",
      "<rootDir>/src/index.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}
